// Q - 1
#include <iostream>
using namespace std;


int main() {
    int n;
    cin >> n;
    
    int arr[n];
    for (int i = 0; i < n; i++){
        cin >> arr[i];
    }
    
    int target;
    cin >> target;
    
    for (int i = 0; i < n; i++){
        if (arr[i] == target){
            cout << "YES";
            return 0;
        }
    }
    cout << "NO";
    return 0;
}
// Q - 2

#include <iostream>
using namespace std;


int main() {
    int n;
    cin >> n;
    
    int arr[n];
    for (int i = 0; i < n; i++){
        cin >> arr[i];
    }
    
    int target;
    cin >> target;
    
    for (int i = 0; i < n; i++){
        if (arr[i] == target){
            cout << i + 1;
            return 0;
        }
    }
    cout << -1;
    return 0;
}
